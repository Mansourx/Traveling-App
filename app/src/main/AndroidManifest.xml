<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.drive">

    <application
        android:allowBackup="true"
        android:icon="@drawable/rent_car_logo_gray"
        android:label="@string/app_name"
        android:roundIcon="@drawable/rent_car_logo_gray"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.example.drive.OnBoardingActivity"></activity>
        <activity android:name="com.example.drive.ProfileActivity" />
        <activity android:name="com.example.drive.SignUpActivity" />
        <activity android:name="com.example.drive.LoginActivity" />
        <activity android:name="com.example.drive.DashBoardActivity" />
        <activity android:name="com.example.drive.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>